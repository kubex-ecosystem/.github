{
  "name": "rafa-mori-portfolio",
  "type": "module",
  "license": "MIT",
  "version": "0.1.0",
  "author": "Rafa Mori",
  "markdown": "github",
  "readme": "README.md",
  "icon": "./public/favicon.ico",
  "homepage": "https://rafa-mori.dev",
  "displayName": "Rafa Mori - Portfolio",
  "description": "Rafa Mori's portfolio website built with Next.js and Tailwind CSS.",
  "packageManager": "pnpm@10.30.3+sha512.c961d1e0a2d8e354ecaa5166b822516668b7f44cb5bd95122d590dd81922f606f5473b6d23ec4a5be05e7fcd18e8488d47d978bbe981872f1145d06e9a740017",
  "private": true,
  "enginesStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/kubex-ecosystem/.github.git"
  },
  "maintainers": [
    {
      "name": "Rafa Mori",
      "email": "faelmori@gmail.com",
      "url": "https://rafa-mori.dev"
    }
  ],
  "bugs": {
    "url": "https://github.com/kubex-ecosystem/.github/issues"
  },
  "cpu": [
    "x64",
    "arm64"
  ],
  "engines": {
    "node": ">=24.x",
    "npm": ">=11.0.0",
    "yarn": ">=1.22.0",
    "vscode": ">=1.80.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "build": "next build",
    "build:static": "next build --export",
    "dev": "next dev --turbopack",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "dev:webpack": "next dev --webpack",
    "dev:webpack-debug": "NODE_OPTIONS='--inspect' next dev --webpack",
    "start": "next start",
    "start:debug": "NODE_OPTIONS='--inspect' next start",
    "serve": "next build && next start",
    "serve:debug": "NODE_OPTIONS='--inspect' next build -d && next start",
    "serve:debug-prerender": "NODE_OPTIONS='--inspect' next build --debug-prerender && next start",
    "serve:profile": "NODE_OPTIONS='--inspect' next build --profile && next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "analyze": "next build --analyze"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.1",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.3.8",
    "@vercel/analytics": "^1.6.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.564.0",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-toastify": "^11.0.5",
    "resend": "^6.9.2",
    "simple-icons": "^16.10.0"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.98.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "jest": "^30.2.0",
    "postcss": "^8.4.38",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3"
  },
  "keywords": [
    "nextjs",
    "tailwindcss",
    "portfolio",
    "react",
    "web-development",
    "frontend",
    "web-design",
    "responsive",
    "modern",
    "minimalist",
    "developer",
    "personal-website",
    "showcase",
    "blog",
    "projects",
    "contact",
    "email",
    "social-media",
    "github",
    "linkedin",
    "twitter",
    "instagram",
    "web-portfolio"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "next/core-web-vitals",
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/no-unescaped-entities": "off",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "no-debugger": "warn",
      "react/jsx-key": "warn",
      "react/jsx-no-undef": "error",
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "react/no-unknown-property": "error",
      "react/no-deprecated": "warn",
      "react/prop-types": "off",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "build",
    "dist",
    "out",
    "coverage",
    "public",
    "next.config.js",
    "postcss.config.js",
    "tailwind.config.js",
    "babel.config.js",
    "jest.config.js",
    "cypress.config.js",
    "cypress",
    "scripts",
    "tests",
    "docs"
  ]
}
