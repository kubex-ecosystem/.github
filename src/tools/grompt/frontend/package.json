    "react-dom": "^19.0.0",
    "react-i18next": "^15.6.0",
    "react-icons": "^4.6.0",
    "react-intersection-observer": "^9.0.0",
    "react-router-dom": "^7.6.3",
    "react-toastify": "^11.0.5",
    "react-tooltip": "^5.29.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
