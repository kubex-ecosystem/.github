    "@types/babel__traverse": "^7.20.7",
    "@types/body-parser": "^1.19.6",
    "@types/bonjour": "^3.5.13",
    "@types/connect": "^3.4.38",
    "@types/connect-history-api-fallback": "^1.5.4",
    "@types/eslint": "^9.6.1",
    "@types/eslint-scope": "^8.3.0",
    "@types/estree": "^1.0.8",
    "@types/express": "^4.17.23",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/graceful-fs": "^4.1.9",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/http-errors": "^2.0.5",
    "@types/http-proxy": "^1.17.16",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/istanbul-reports": "^3.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/json-schema": "^7.0.15",
    "@types/json5": "^0.0.30",
    "@types/mime": "^3.0.4",
    "@types/node": "^20.19.4",
    "@types/node-forge": "^1.3.12",
    "@types/parse-json": "^4.0.2",
    "@types/prettier": "^2.7.3",
    "@types/q": "^1.5.8",
    "@types/qs": "^6.14.0",
    "@types/range-parser": "^1.2.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/resolve": "^1.20.6",
    "@types/retry": "^0.12.5",
    "@types/semver": "^7.7.0",
    "@types/send": "^0.17.5",
    "@types/serve-index": "^1.9.4",
    "@types/serve-static": "^1.15.8",
    "@types/sockjs": "^0.3.36",
    "@types/stack-utils": "^2.0.3",
    "@types/tough-cookie": "^4.0.5",
    "@types/trusted-types": "^2.0.7",
    "@types/ws": "^8.18.1",
    "@types/yargs": "^17.0.33",
    "@types/yargs-parser": "^21.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^8.9.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "lint-staged": "^16.1.2",
    "prettier": "^3.1.0",
    "serve": "^14.0.1",
    "typescript": "^5.8.3",
    "web-vitals": "^5.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "prompt-engineering",
    "ai",
    "claude",
    "react",
    "golang",
    "embedded",
    "tailwindcss",
    "portfolio",
    "react",
    "web-development",
    "frontend",
    "web-design",
    "responsive",
    "modern",
    "minimalist",
    "developer",
    "personal-website",
    "showcase",
    "projects",
    "contact",
    "social-media",
    "github"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react-dom": "^19.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": true,
    "tabWidth": 2,
    "printWidth": 80,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "useTabs": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "htmlWhitespaceSensitivity": "css",
    "proseWrap": "preserve",
    "embeddedLanguageFormatting": "auto",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/dist/",
      "/out/",
      "/coverage/",
      "/public/",
      "/next.config.js",
      "/postcss.config.js",
      "/tailwind.config.js",
      "/babel.config.js",
      "/jest.config.js",
      "/cypress.config.js",
      "/cypress/",
      "/scripts/",
      "/tests/",
      "/docs/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json",
      "text",
      "lcov",
      "clover"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/dist/",
      "/out/",
      "/public/",
      "/next.config.js",
      "/postcss.config.js",
      "/tailwind.config.js",
      "/babel.config.js",
      "/jest.config.js",
      "/cypress.config.js",
      "/cypress/",
      "/scripts/",
      "/tests/",
      "/docs/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<root_dir>/__mocks__/fileMock.js",
      "\\.(mp4|webm|ogg)$": "<root_dir>/__mocks__/fileMock.js",
      "\\.(woff|woff2|eot|ttf|otf|svg)$": "<root_dir>/__mocks__/fileMock.js"
    }
  }
}
